\STATE $N \Leftarrow N / 2$
\ELSE[$N$ is odd]
\STATE $y \Leftarrow y \times X$
\STATE $N \Leftarrow N - 1$
\ENDIF
\ENDWHILE
\end{algorithmic}
\caption{Calculate $y = x^n$}          % give the algorithm a caption
\label{alg1}
\end{algorithm}

